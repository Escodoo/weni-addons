    <record id="view_mis_cash_flow_forecast_line_pivot" model="ir.ui.view">
        <field name="name">mis.cash_flow.forecast_line.pivot (in weni_mis_builder_cash_flow_custom)</field>
        <field name="model">mis.cash_flow.forecast_line</field>
        <field name="type">pivot</field>
        <field name="arch" type="xml">
            <pivot string="Forecast">
                <field name="account_id" type="row"/>
                <field name="partner_id" type="row"/>
                <field name="date" type="col" interval="month"/>
                <field name="balance" type="measure"/>
            </pivot>
        </field>
    </record>

    <record id="view_mis_cash_flow_forecast_line_graph" model="ir.ui.view">
        <field name="name">mis.cash_flow.forecast_line.graph (in weni_mis_builder_cash_flow_custom)</field>
        <field name="model">mis.cash_flow.forecast_line</field>
        <field name="arch" type="xml">
            <graph string="Forecast" stacked="True" type="bar">
                <field name="date" type="row" interval="month"/>
                <field name="account_id" type="row"/>
                <field name="balance" type="measure"/>
            </graph>
        </field>
    </record>

    <record id="mis_builder_cash_flow.action_mis_cash_flow_forecast_line" model="ir.actions.act_window">
        <field name="view_mode">tree,graph,pivot,form</field>
    </record>


