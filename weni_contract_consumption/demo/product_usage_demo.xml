<odoo noupdate="1">
    <!-- Product Usages -->
    <record id="product_usage_demo_1" model="product.usage">
        <field name="name">Product Usage Demo 1</field>
        <field name="partner_id" ref="base.res_partner_1" />
        <field name="weni_id">WENI12345</field>
        <field name="project_uid">PROJ123</field>
        <field name="project_name">Demo Project 1</field>
        <field
            name="date"
            eval="(DateTime.now() - timedelta(days=7)).strftime('%Y-%m-%d')"
        />
        <field name="active_user_count">15</field>
        <field name="channel_uuid">CHAN123</field>
        <field name="channel_name">Demo Channel 1</field>
        <field name="channel_type">Text</field>
        <field name="channel_message_count">120</field>
        <field name="flow_uuid">FLOW123</field>
        <field name="flow_name">Demo Flow 1</field>
        <field name="flow_message_count">80</field>
        <field name="ia_connect_uuid">IACON123</field>
        <field name="ia_connect_name">Demo IA Connect 1</field>
        <field name="ia_connect_prediction_count">50</field>
        <field name="human_service_department">Customer Support</field>
        <field name="human_service_qrd_service_count">30</field>
        <field name="human_agent_count">5</field>
        <field name="login_count">25</field>
    </record>

